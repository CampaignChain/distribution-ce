{
    "name": "campaignchain/distribution-ce",
    "description": "Community Edition of CampaignChain.",
    "keywords": ["campaignchain","community edition", "ce", "distribution"],
    "type": "campaignchain-distribution",
    "homepage": "http://www.campaignchain.com",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Sandro Groganz",
            "email": "sandro@campaignchain.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "campaignchain/core": "dev-1.0.0-beta.1",
        "campaignchain/campaign-scheduled": "dev-1.0.0-beta.1",
        "campaignchain/milestone-scheduled": "dev-1.0.0-beta.1",
        "campaignchain/report-analytics-metrics-per-activity": "dev-1.0.0-beta.1",
        "campaignchain/doc-html": "1.0.0-alpha.3"
    },
    "autoload": {
        "psr-4": { "CampaignChain\\DistributionCeBundle\\": "" }
    },
    "extra": {
        "campaignchain": {
            "kernel": {
                "classes": [
                    "CampaignChain\\DistributionCeBundle\\CampaignChainDistributionCeBundle",
                    "CampaignChain\\DocHtmlBundle\\CampaignChainDocHtmlBundle"
                ]
            }
        }
    }
}
